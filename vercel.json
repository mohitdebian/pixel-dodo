{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["api/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.js",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/dodo-redirect",
      "dest": "/api/webhook.js",
      "methods": ["GET", "POST", "OPTIONS"]
    }
  ]
} 